{
	"name": "spacepoints",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@material-ui/core": "^4.2.1",
		"@material-ui/icons": "^4.2.1",
		"firebase": "^6.3.1",
		"prop-types": "^15.7.2",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"react-router-dom": "^5.0.1",
		"react-scripts": "3.0.1"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "kcd-scripts test",
		"text:cover": "kcd-scripts test --coverage",
		"lint": "kcd-scripts lint --config .eslintrc.js --ignore-path .eslintignore",
		"format": "prettier --write \"**/*.+(js|json|css|md|mdx|html)\"",
		"validate": "kcd-scripts validate lint,test:cover"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@testing-library/dom": "^5.6.0",
		"@testing-library/jest-dom": "^4.0.0",
		"@testing-library/react": "^8.0.5",
		"kcd-scripts": "^1.5.2",
		"prettier": "^1.18.2"
	}
}
